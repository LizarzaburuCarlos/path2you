{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "date": {
      "type": "datetime"
    },
    "score": {
      "type": "integer",
      "min": 0,
      "max": 10,
      "required": true,
      "default": 10
    },
    "comment": {
      "type": "text",
      "default": "Rese√±a sin Comentario",
      "minLength": 3,
      "required": false
    }
  }
}
