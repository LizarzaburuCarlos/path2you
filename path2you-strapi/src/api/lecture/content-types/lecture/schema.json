{
  "kind": "collectionType",
  "collectionName": "lectures",
  "info": {
    "singularName": "lecture",
    "pluralName": "lectures",
    "displayName": "Lecture",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "default": "Lectura sin Nombre",
      "required": true,
      "minLength": 3
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "lectures"
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "lessons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson.lesson",
      "mappedBy": "lecture"
    },
    "description": {
      "type": "richtext"
    }
  }
}
